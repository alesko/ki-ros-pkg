<package>
  <description brief="labjack">

    ROS driver for labjack UBS data A/D and D/A converter.

  </description>
  <author>Alexander Skoglund</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/labjack</url>
  <depend package="std_msgs"/>
  <depend package="rospy"/>
  <depend package="roscpp"/>
  <rosdep name="labjackusb"/>
  <export>
    <cpp cflags="-I${prefix}/msg/cpp -I${prefix}/srv/cpp" lflags="-L${prefix}/lib -lm -L${prefix}/usr/local/lib "/>
  </export>
</package>


