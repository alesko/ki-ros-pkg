<package>
  <description brief="shadow">

     ROS interface for the Shadow Pneumatic Control Unit (SPUC). 

  </description>
  <author>Alexander Skoglund</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/shadow</url>
  <depend package="std_msgs"/>
  <depend package="roscpp"/>
  <depend package="tf"/>
  <depend package="sensor_msgs"/>
  <depend package="control_toolbox"/>
  <depend package="eigen"/>
  <depend package="dataglove"/>
  <rosdep name="gtk2"/>
  <rosdep name="gsl"/>
  <export>
    <cpp cflags="-I${prefix}/msg/cpp -I${prefix}/srv/cpp -I/usr/include/gsl" lflags="-L${prefix}/lib -lgsl -lgslcblas -lm "/>
  </export>

</package>


